<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whale.admin.web.repository.mapper.UserMapper">

    <select id="findByName" resultType="com.whale.admin.web.repository.model.User">
        SELECT * FROM user WHERE NAME = #{name}
    </select>

    <insert id="insert">
        INSERT INTO user(NAME, AGE) VALUES(#{name}, #{age})
    </insert>

</mapper>
